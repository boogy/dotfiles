# Show comments at right of disassembly if they fit in screen
e asm.cmtright=true

# Shows pseudocode in disassembly. Eg mov eax, str.ok = > eax = str.ok
e asm.pseudo = false

# Display stack and register values on top of disasembly view (visual mode)
e cmd.stack = true

# Solarized theme
eco solarized

# Use UTF-8 to show cool arrows that do not look like crap :)
e scr.utf8 = true

eval scr.color = true
eval dbg.bep   = entry
eval file.id   = true
eval file.flag = true
eval file.analyze = true

# Show stack and regs in visual mode in a slow but verbose mode
e dbg.slow = true

# Use uppercase syntax at disassembly
e asm.ucase = true
